<script>
   $(document).ready(function(){
      $('#tabShow').hide();
      $('#tabToggle').click(function(){
         $('#tabShow').toggle(); 
         $(this).hide();
      });

      var reviewRating = 0;
      var userid = Number("0");
      var tutorid = Number("34230");
      $('#btnReviewPost').click(function(){
         var reviewContent = $('#txtReviewContent').val();        
         if($("input[name=reviewRating]").is(':checked')){
            reviewRating = $("input[name=reviewRating]:checked").val();
         }
         
         if(reviewContent == ''){            
            $('#errorReview').html('Please enter review content');
            $('#txtReviewContent').focus();
            $('#txtReviewContent').css('border-color','red');
            return false;
         }
         
         else if (reviewRating == 0)
         { 
            $('#txtReviewContent').css('border-color','#ccc');
            $('#errorReview').html('Please select rating');
            $('.rated').focus();
            $('.rated').css('border-color','red');           
            return false;
         }
         
         $('#errorReview').html('');
         $('.rated').css('border-color','#ccc');  
         $.ajax({
            url: baseurl+"ajax/ajaxPostTutorReview.php",
            method:"POST",
            data: {"reviewContent":reviewContent,'reviewRating':reviewRating,'tutorid':tutorid}         
         }).done(function(msg) { 
            if(msg == -99)
            {
               window.location.href = baseurl+ 'index.php';
            }
            else if(msg === 0)
            {
               alert('Something went wrong! Please try after sometime.');
            }
            else if(msg == 2)
            {
               $('#tabShow').hide();
               $('#txtReviewContent').val('');
               reviewRating = 0;
               $("input[name=reviewRating]").attr('checked', false);
               alert('You have already given review for this tutor.');               
            } else {
               alert('Review Submitted Successfully.');
               $('#txtReviewContent').val('');
               reviewRating = 0;
               $("input[name=reviewRating]").attr('checked', false);
               $('#tabShow').hide();
              // $('#tabToggle').show();
            }
         });
      }); 
         
      $(".screenshot").mouseenter(function(e){
        $('.maximize').show();
      });
       
      $(".screenshot").click(function(e){            
        var photo = $(this).data('photo');
        var floder = $(this).data('floder');
        var tobj = this;
        $.ajax({               
			   url: baseurl+"ajax/ajaxPhotoEnlarge.php",
			   method:"POST",
			   data: {"photo":photo,"floder":floder}
			}).done(function(msg) {            
			   $(tobj).next().html('<img src="'+msg+'" />');	
            $(".photoEnlagreDive").hide();
         
            $(tobj).next().show();            
			}); 
      });
      
      
      $('.photoEnlagreDive').on('mouseenter', function(e) {
         $(".maximize").show();  
         var $el = $(".photoEnlagreDive");
         $(this).show();
      });
      
      
      $('.screenshot,.photoEnlagreDive').on('mouseleave', function(e) {
          $(".maximize").hide();  
         var $el = $(".photoEnlagreDive");
         if ($el.is(":visible")) {
            $el.hide();
         } 
      });
      
      
      $(document).on('click', function(e) {
         var $el = $(".photoEnlagreDive");         
         if ($el.is(":visible")) {
            $el.hide();
         } 
      });
      
     /*  
      function readCookie(name) {
         var nameEQ = name + "=";
         var ca = document.cookie.split(';');
         for (var i = 0; i < ca.length; i++) {
             var c = ca[i];
             while (c.charAt(0) == ' ') c = c.substring(1, c.length);
             if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
         }
         return 0;         
      }
      */
      
      var cookie_tutor_send_user = readCookie("tp_tutor_send_user");
      var cookie_tutor_send_name = readCookie("tp_tutor_send_name");
      //console.log(userid);
      //console.log(cookie_tutor_send_user);
      if(userid != 0){
         if(cookie_tutor_send_user != 0 ){
            $('.sendmsg').trigger('click');
         }
      }      
      
   });
</script>
