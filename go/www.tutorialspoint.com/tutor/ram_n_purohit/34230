    $(document).ready(function(){
       $('#searchSubmit').click(function(){
         var searchKey = $('#txtSearchText').val();
         if(searchKey == ''){
            $('#searchError').html('Please enter search keyword');
            $('#txtSearchText').focus();
            $('#txtSearchText').css('border-color','red');
            $('#searchError').css('color','red');
            return false;
         }
         $('#txtSearchText').css('border-color','#ccc');
         $('#searchError').html('');
       });
    });
    
